@component-name = "portal-knowledge-base"
definition {

	property custom.properties = "default.layout.template.id=1_column";
	property portal.release = "true";
	property portal.upstream = "true";
	property testray.main.component.name = "Knowledge Base";

	setUp {
		TestCase.setUpPortalInstance();

		User.firstLoginPG();
	}

	tearDown {
		var testPortalInstance = PropsUtil.get("test.portal.instance");

		if ("${testPortalInstance}" == "true") {
			PortalInstances.tearDownCP();
		}
		else {
			Workflow.tearDownSiteWorkflow();

			KBFolder.tearDownCP();

			KBArticle.tearDownCP();

			JSONUser.tearDownNonAdminUsers();

			Page.tearDownCP();
		}
	}

	@priority = "3"
	@refactorneeded
	test AddKBChildArticleAsDraft {
		JSONLayout.addPublicLayout(
			groupName = "Guest",
			layoutName = "Knowledge Base Page");

		JSONLayout.addWidgetToPublicLayout(
			column = "1",
			groupName = "Guest",
			layoutName = "Knowledge Base Page",
			widgetName = "Knowledge Base Display");

		KBAdmin.openKBAdmin(siteURLKey = "guest");

		KBArticle.addCP(
			kbArticleContent = "Knowledge Base Parent Article Content",
			kbArticleTitle = "Knowledge Base Parent Article Title");

		Navigator.gotoPage(pageName = "Knowledge Base Page");

		KBArticle.addChildPGViaKBDisplay(
			kbArticleContent = "Knowledge Base Child Article Content",
			kbArticleTitle = "Knowledge Base Child Article Title",
			saveAsDraft = "true");

		Pause(locator1 = "3000");

		Navigator.gotoPage(pageName = "Knowledge Base Page");

		KBArticle.viewPG(
			kbArticleContent = "Knowledge Base Parent Article Content",
			kbArticleTitle = "Knowledge Base Parent Article Title");

		AssertElementNotPresent(locator1 = "KnowledgeBase#HIERARCHY_NAVIGATION_CHILD");

		KBAdmin.openKBAdmin(siteURLKey = "guest");

		KBArticle.gotoChildArticleDescriptiveDetails(kbArticleTitle = "Knowledge Base Parent Article Title");

		KBArticle.viewCP(
			kbArticleContent = "Knowledge Base Child Article Content",
			kbArticleTitle = "Knowledge Base Child Article Title",
			kbStatus = "Draft");
	}

	@description = "This test covers LPS-91303. It ensures that the configured KB root folder is available for selection."
	@priority = "3"
	@refactorneeded
	test ConfiguredKBRootFolder {
		JSONLayout.addPublicLayout(
			groupName = "Guest",
			layoutName = "Knowledge Base Display Page");

		JSONLayout.addWidgetToPublicLayout(
			column = "1",
			groupName = "Guest",
			layoutName = "Knowledge Base Display Page",
			widgetName = "Knowledge Base Display");

		KBAdmin.openKBAdmin(siteURLKey = "guest");

		KBArticle.addCP(
			kbArticleContent = "Knowledge Base Home Article",
			kbArticleTitle = "Knowledge Base Home Article");

		KBAdmin.openKBAdmin(siteURLKey = "guest");

		KBFolder.addCP(
			kbFolderDescription = "KB Folder 1 Description",
			kbFolderName = "KB Folder 1");

		KBFolder.gotoCP(kbFolderName = "KB Folder 1");

		KBArticle.addCP(
			kbArticleContent = "Knowledge Base Article Folder 1 Content 1",
			kbArticleTitle = "Knowledge Base Article Folder 1 Title 1");

		KBAdmin.openKBAdmin(siteURLKey = "guest");

		KBFolder.addCP(
			kbFolderDescription = "KB Folder 2 Description",
			kbFolderName = "KB Folder 2");

		KBFolder.gotoCP(kbFolderName = "KB Folder 2");

		KBArticle.addCP(
			kbArticleContent = "Knowledge Base Article Folder 2 Content 1",
			kbArticleTitle = "Knowledge Base Article Folder 2 Title 1");

		Navigator.gotoPage(pageName = "Knowledge Base Display Page");

		KBFolder.gotoViaKBDisplayPG(kbFolderName = "KB Folder 1");

		KBArticle.viewViaKBDisplayPG(
			kbArticleContent = "Knowledge Base Article Folder 1 Content 1",
			kbArticleTitle = "Knowledge Base Article Folder 1 Title 1");

		KBFolder.gotoViaKBDisplayPG(kbFolderName = "Home");

		KBArticle.viewViaKBDisplayPG(
			kbArticleContent = "Knowledge Base Home Article",
			kbArticleTitle = "Knowledge Base Home Article");

		KBFolder.gotoViaKBDisplayPG(kbFolderName = "KB Folder 2");

		KBArticle.viewViaKBDisplayPG(
			kbArticleContent = "Knowledge Base Article Folder 2 Content 1",
			kbArticleTitle = "Knowledge Base Article Folder 2 Title 1");
	}

