    <td>DATE_PICKER</td>
    <td>//div[@class='date-picker-root']</td>
    <td></td>
  </tr>
  <tr>
    <td>DATE_PICKER_MONTH_DROPDOWN</td>
    <td>//select[@class='date-picker-select-root form-control'][1]</td>
    <td></td>
  </tr>
  <tr>
    <td>DATE_PICKER_YEAR_DROPDOWN</td>
    <td>//select[@class='date-picker-select-root form-control'][2]</td>
    <td></td>
  </tr>
  <tr>
    <td>DATE_PICKER_DROPDOWN_OPTION</td>
    <td>//TODO</td>
    <td></td>
  </tr>
  <tr>
    <td>DATE_PICKER_DAY</td>
    <td>//table[@class='calendar-root']//button[@class='button-root btn btn-unstyled day-root' and text()='${key_day}']"</td>
    <td></td>
  </tr>
  </tbody>
</table>
</body>
</html>
