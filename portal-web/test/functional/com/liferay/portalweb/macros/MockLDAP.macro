definition {

	macro selectJsonFile {
		Navigator.gotoNavTab(navTab = "${tabName}");

		var dependenciesDir = TestCase.getDependenciesDirPath();
		var filePath = "${dependenciesDir}/${fileName}";

		UploadFile(
			key_tab = "${tabName}",
			locator1 = "MockLDAP#BROWSE_BUTTON",
			value1 = "${filePath}");

		AssertClick(
			key_tab = "${tabName}",
			locator1 = "Button#TAB_SUBMIT_BTN",
			value1 = "Add ${tabName}");

		Alert.viewSuccessMessage();
	}

}