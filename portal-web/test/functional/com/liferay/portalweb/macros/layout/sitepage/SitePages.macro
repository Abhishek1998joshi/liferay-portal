definition {

	macro getFriendlyURL {
		var baseURL = "${baseURL}";

		if (contains("${baseURL}", "{baseURL}")) {
			var baseURL = PropsUtil.get("portal.url");
		}

		var key_friendlyURL = "";
		var friendlyURL = selenium.getElementValue("SitePages#FRIENDLY_URL");

		return "${baseURL}/web/guest${friendlyURL}";
	}

}