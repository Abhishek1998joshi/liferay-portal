definition {

	macro click {
		Click(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

	macro clickNoError {
		ClickNoError(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

	macro viewNotPresent {
		AssertElementNotPresent(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

	macro viewOrderMenuActionOnWebContent {
		var key_index = "1";

		for (var listMenuItem : list "${listMenuItemNames}") {
			if ("${listMenuItem}" != "-") {
				AssertTextEquals(
					index = "${key_index}",
					locator1 = "MenuItem#DROPDOWN_MENU_ITEM_SPECIFIC",
					value1 = "${listMenuItem}");
			}

			var key_index = ${key_index} + 1;
		}
	}

	macro viewPresent {
		AssertElementPresent(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

	macro viewVisible {
		AssertVisible(
			key_menuItem = "${menuItem}",
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

}