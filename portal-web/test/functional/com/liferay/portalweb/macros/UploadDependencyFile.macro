definition {

	macro uploadArchiveFile {
		var tempDir = PropsUtil.get("output.dir.name");

		var dependenciesDir = TestCase.getDependenciesDirPath();
		var filePath = "${dependenciesDir}/${fileName}";
		var fileExists = FileUtil.exists("${filePath}");

		if (("${fileExists}" == "true") && ("${globalDependenciesDir}" != "true")) {
			if (!(isSet(compressedFile))) {
				ArchiveUtil.archive("${filePath}", "${tempDir}/${fileName}");

				var filePath = "${tempDir}/${fileName}";
			}

			UploadFile(
				locator1 = "TextInput#FILE",
				value1 = "${filePath}");
		}
		else {
			UploadCommonFile.uploadCommonFileHiddenNoMouseOver(
				locator1 = "TextInput#FILE",
				value1 = "${fileName}");
		}
	}

	macro uploadFile {
		var dependenciesDir = TestCase.getDependenciesDirPath();
		var filePath = "${dependenciesDir}/${fileName}";
		var fileExists = FileUtil.exists("${filePath}");

		if (("${fileExists}" == "true") && ("${globalDependenciesDir}" != "true")) {
			UploadFile(
				locator1 = "TextInput#FILE",
				value1 = "${filePath}");
		}
		else {
			UploadCommonFile.uploadCommonFileHiddenNoMouseOver(
				locator1 = "TextInput#FILE",
				value1 = "${fileName}");
		}
	}

}