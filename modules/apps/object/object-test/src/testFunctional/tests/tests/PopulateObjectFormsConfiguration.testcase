			fieldType = "String");

		PopulateObjectFormsConfiguration.closeUnmappedError();

		PopulateObjectFormsConfiguration.saveFormObject();

		PopulateObjectFormsConfiguration.viewUnmappedObjectRequiredFieldsError(
			fieldName = "text",
			fieldType = "String");
	}

