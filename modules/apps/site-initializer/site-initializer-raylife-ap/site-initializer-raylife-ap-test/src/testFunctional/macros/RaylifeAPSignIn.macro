definition {

	macro signInRaylifeAP {
		if (!(isSet(emailAddress))) {
			var emailAddress = "test@liferay.com";
		}

		if (!(isSet(password))) {
			var password = "test";
		}

		Type(
			locator1 = "RaylifeAP#EMAIL_LOGIN_FIELD",
			value1 = "${emailAddress}");

		Type(
			locator1 = "RaylifeAP#PASSWORD_LOGIN_FIELD",
			value1 = "${password}");

		Click(locator1 = "RaylifeAP#SIGN_IN_BUTTON");
	}

}