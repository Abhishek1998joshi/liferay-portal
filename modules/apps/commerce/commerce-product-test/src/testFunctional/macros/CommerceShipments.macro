definition {

	macro assertShipmentItemWarehouseAndQuantityInShipment {
		AssertTextEquals(
			key_columnNumber = "4",
			key_entryName = "${shipmentItemSku}",
			locator1 = "CommerceEntry#TABLE_CELL_BY_COLUMN_ORDER",
			value1 = "${warehouseName}");

		AssertTextEquals(
			key_columnNumber = "6",
			key_entryName = "${shipmentItemSku}",
			locator1 = "CommerceEntry#TABLE_CELL_BY_COLUMN_ORDER",
			value1 = "${quantityInShipment}");
	}

}