<?xml version="1.0"?>

<taglib
	version="2.1"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd"
>
	<description>Provides the Liferay Friendly URL component tags, prefixed with <![CDATA[<code>liferay-friendly-url:</code>]]>.</description>
	<tlib-version>1.0</tlib-version>
	<short-name>liferay-friendly-url</short-name>
	<uri>http://liferay.com/tld/friendly-url</uri>
	<tag>
		<description>Attaches the Friendly URL History management button to the provided element.</description>
		<name>history</name>
		<tag-class>com.liferay.friendly.url.taglib.servlet.taglib.FriendlyURLHistoryTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>className</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>classPK</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>elementId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>